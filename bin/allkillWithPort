    kill -s KILL $pid
done

#  killall -q -9 FactoryServer
list=`ps -f U $USER --width 1000 | grep FactoryServer | grep -v grep |  awk '{print $2}'` 
for pid in $list; do
    kill -s KILL $pid
done

#  killall -q -9 SuperVisionContainer
list=`ps -f U $USER --width 1000 | grep SuperVisionContainer | grep -v grep |  awk '{print $2}'` 
