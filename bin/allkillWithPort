for pid in $list; do
    kill -s KILL $pid
done

#  killall -q -9 geom
list=`ps -e -o pid,user,fname | awk '{if ($2 == USRNAME && $3 == FNAME) print $1}' USRNAME=$USER FNAME=geom`
for pid in $list; do
    kill -s KILL $pid
done

#  killall -q -9 SALOMEDS_Server
