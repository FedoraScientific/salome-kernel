top_srcdir=@top_srcdir@
top_builddir=..
srcdir=@srcdir@
VPATH=.:@srcdir@
curdir = $(shell pwd)
instdatadir=${prefix}/examples

SUBDIRS=.

@COMMENCE@

data:
	(cd $(srcdir) ; cp -fr `ls | grep -v "Makefile.in"` $(curdir))

clean:
	 rm -rf `ls | grep -v "Makefile"`  

install:
	rm -rf $(instdatadir)
	mkdir $(instdatadir)
	cp -rf `ls | grep -v "Makefile"` $(instdatadir) 

uninstall:
	rm -rf $(instdatadir)
